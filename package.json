{
  "name": "pambazo-bakery-management",
  "version": "1.0.0",
  "description": "Sistema completo de gestión para panadería optimizado para móvil con PWA",
  "main": "App.tsx",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "pwa-test": "vite build && vite preview --port 4173",
    "clean": "rm -rf dist node_modules",
    "install-clean": "npm run clean && npm install",
    "update-deps": "npm update && npm audit fix",
    "client:dev": "vite",
    "server:dev": "nodemon",
    "server:build": "tsc --project api",
    "server:start": "node api/dist/server.js",
    "server:secure": "node api-server-secure.cjs",
    "server:websocket": "node api-server-websocket.cjs",
    "start:prod": "node api/dist/server.js",
    "migrate": "node scripts/run-migrations.js",
    "db:setup": "npm run migrate",
    "security:test": "node scripts/test-security.cjs",
    "test:final": "node test-final-complete.cjs",
    "check": "npm run type-check && npm run lint"
  },
  "keywords": [
    "bakery",
    "management",
    "pwa",
    "mobile",
    "react",
    "typescript",
    "tailwind",
    "pos-system",
    "inventory-management",
    "order-management"
  ],
